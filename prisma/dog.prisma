model Dog {
    id        Int      @id @default(autoincrement())
    name      String
    breed     String
    size      Size
    age       Int
    weight    Float    @default(0)
    color     String   @default("Non specified")
    imageUrl  String?
    notes     String?
    owner     User?    @relation(fields: [ownerId], references: [id])
    ownerId   Int?
    companyId Int?
    company   Company? @relation("CompanyAlumni", fields: [companyId], references: [id])
}

enum Size {
    TOY
    SMALL
    MEDIUM
    LARGE
    GIGANTIC
}
